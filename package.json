{
  "name": "emoji-poll",
  "version": "0.0.1",
  "description": "Emoji poll for Emojicon Hack",
  "engines": {
    "node": "5.9.1"
  },
  "main": "index.js",
  "scripts": {
    "start": "nodemon index.js",
    "build-css": "node-sass --include-path scss src/styles/main.scss public/css/main.css",
    "watch-css": "nodemon -e scss -x \"npm run build-css\"",
    "postinstall": "npm run build-css"
  },
  "dependencies": {
    "bluebird": "^3.4.6",
    "body-parser": "^1.15.2",
    "ejs": "2.4.1",
    "express": "4.13.3",
    "mongoose": "^4.6.6",
    "pug": "^0.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/psiemens/emoji-poll"
  },
  "keywords": [
    "node",
    "heroku",
    "express"
  ],
  "license": "MIT",
  "devDependencies": {
    "bulma": "^0.2.3",
    "node-sass": "^3.11.2"
  }
}
